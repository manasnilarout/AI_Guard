    "axios-retry": "^4.0.0",
    "dotenv": "^16.3.0",
    "joi": "^17.11.0",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.4.0",
    "koa-compress": "^5.1.0",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/koa": "^2.13.0",
    "@types/koa__cors": "^5.0.0",
