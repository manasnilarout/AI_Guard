{
  "name": "ai-guard",
  "version": "1.0.0",
  "description": "Universal AI Proxy Service",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server/index.ts",
    "build": "esbuild src/server/index.ts --bundle --platform=node --outdir=dist",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit"
